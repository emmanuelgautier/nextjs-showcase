{
  "name": "with-react-intl",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build",
    "export": "next export",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "start": "next start"
  },
  "dependencies": {
    "@formatjs/cli": "4.8.2",
    "@formatjs/intl-datetimeformat": "4.5.2",
    "@formatjs/intl-getcanonicallocales": "1.9.0",
    "@formatjs/intl-numberformat": "7.4.2",
    "@formatjs/intl-pluralrules": "4.3.2",
    "@formatjs/intl-relativetimeformat": "9.5.2",
    "accepts": "1.3.8",
    "autoprefixer": "10.4.2",
    "babel-plugin-react-intl": "8.2.25",
    "glob": "7.2.0",
    "next": "latest",
    "next-seo": "4.29.0",
    "postcss": "8.4.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.6",
    "tailwindcss": "3.0.23"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "2.5.1",
    "typescript": "4.6.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid"
  }
}
