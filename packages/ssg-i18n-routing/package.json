{
  "name": "with-react-intl",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build",
    "export": "next export",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "start": "next start"
  },
  "dependencies": {
    "@formatjs/cli": "6.3.14",
    "@formatjs/intl-datetimeformat": "6.17.1",
    "@formatjs/intl-getcanonicallocales": "2.5.4",
    "@formatjs/intl-numberformat": "8.15.1",
    "@formatjs/intl-pluralrules": "5.4.1",
    "@formatjs/intl-relativetimeformat": "11.4.8",
    "accepts": "1.3.8",
    "autoprefixer": "10.4.4",
    "babel-plugin-react-intl": "8.2.25",
    "glob": "10.3.3",
    "next": "latest",
    "next-seo": "6.1.0",
    "postcss": "8.4.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-intl": "7.0.4",
    "tailwindcss": "3.0.23"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "3.0.1",
    "typescript": "5.0.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid"
  }
}
