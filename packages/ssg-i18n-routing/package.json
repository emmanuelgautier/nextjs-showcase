{
  "name": "with-react-intl",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node --project tsconfig.server.json server.ts",
    "dev-no-custom-server": "next dev",
    "build": "npm run extract:i18n && npm run compile:i18n && next build && tsc -p tsconfig.server.json",
    "export": "next export",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node dist/server",
    "start-no-custom-server": "next start"
  },
  "dependencies": {
    "@formatjs/cli": "2.15.0",
    "@formatjs/intl-datetimeformat": "2.8.4",
    "@formatjs/intl-getcanonicallocales": "1.5.3",
    "@formatjs/intl-numberformat": "5.7.6",
    "@formatjs/intl-pluralrules": "3.5.6",
    "@formatjs/intl-relativetimeformat": "7.3.6",
    "accepts": "1.3.7",
    "autoprefixer": "10.2.4",
    "babel-plugin-react-intl": "8.2.25",
    "cross-env": "7.0.3",
    "full-icu": "1.3.1",
    "glob": "7.1.6",
    "next": "latest",
    "next-seo": "4.20.0",
    "postcss": "8.2.6",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.12.3",
    "tailwindcss": "2.0.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/accepts": "1.3.5",
    "cross-spawn": "7.0.3",
    "prettier": "2.2.1",
    "ts-node": "8.0.1",
    "typescript": "4.0.7"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid"
  }
}
